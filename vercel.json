{
  "version": 2,
  "public": true,
  "functions": {
    "api/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/render",
      "dest": "/api/render.js"
    },
    {
      "src": "/diagnose",
      "dest": "/api/diagnose.js"
    },
    {
      "src": "/icp-widget.js",
      "dest": "/public/icp-widget.js"
    }
  ]
}
